<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.forums.model.ForumsVO" table="forums" >
        
        <id name="forno" column="forno">
            <generator class="sequence" >
              <param name="sequence">forums_seq</param>
            </generator>  
        </id>
        <property name="memno" column="memno" />
        <property name="forclass" column="forclass" />
        <property name="fortheme" column="fortheme" />
        <property name="forcontent" column="forcontent" />
        <property name="fortime" column="fortime" />
    </class>
</hibernate-mapping>

