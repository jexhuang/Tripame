<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.forumsrep.model.ForumsrepVO" table="forumsrep" >
        
        <id name="repno" column="repno">
            <generator class="sequence" >
              <param name="sequence">forumsrep_seq</param>
            </generator>  
        </id>
        <property name="forno" column="forno" />
        <property name="memno" column="memno" />
        <property name="repcontent" column="repcontent" />
        <property name="reptime" column="reptime" />
    </class>
</hibernate-mapping>

